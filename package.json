{
  "name": "cc-modules",
  "version": "0.0.4",
  "description": "A Cocos Creator Module Manager",
  "author": "aztack <aztack@163.com>",
  "main": "index.js",
  "main-menu": {
    "i18n:MAIN_MENU.package.title/i18n:cc-modules.title": {
      "command": "Editor.Panel.open",
      "params": [
        "cc-modules"
      ]
    }
  },
  "panel": {
    "main": "panel/index.js",
    "type": "dockable",
    "title": "i18n:cc-modules.title",
    "width": 300,
    "height": 800
  },
  "profiles": {
    "project": {
      "docs": "http://docs.cocos.com/creator/manual/zh/",
      "forum": "http://forum.cocos.com/c/Creator"
    },
    "local": {}
  },
  "dependencies": {
    "extract-zip": "^1.6.7",
    "shelljs": "^0.8.3"
  },
  "runtime-resource": {
    "path": "shared",
    "name": "shared"
  },
  "reload": {
    "ignore": [
      "shared/**/*"
    ]
  }
}
